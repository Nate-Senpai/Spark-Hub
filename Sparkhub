local Page = UI.New({
    Title = "Era of Althea"
})
Page.Button({
    Text = "Buy Health Potion",
    Callback = function()
       local args = {
    [1] = workspace.Purchases.Items:FindFirstChild("Large Health Potion")
}
game:GetService("ReplicatedStorage").Events.PurchaseItem:FireServer(unpack(args))
end
})
Page.Button({
    Text = "Buy Mana Potion",
    Callback = function()
  local args = {
    [1] = workspace.Purchases.Items:FindFirstChild("Large Mana Potion")
}
game:GetService("ReplicatedStorage").Events.PurchaseItem:FireServer(unpack(args))
end
})
Page.Button({
    Text = "Buy Teleport Potion",
    Callback = function()
  local args = {
    [1] = workspace.Purchases.Items:FindFirstChild("Teleport Crystal")
}
game:GetService("ReplicatedStorage").Events.PurchaseItem:FireServer(unpack(args))
end
})
